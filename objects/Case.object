    <validationRules>
        <fullName>Limit_Priority</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
     /* $Profile.Name =     Nome da profile  
     $CustomMetadata.Profiles__mdt.Sales.ProfileName__c  , /* CMDT com os nomes das profiles a serem &quot;barradas&quot; */ 
     
     RecordType.DeveloperName = &quot;Inquiry&quot;,  /* Nome do record type */
     $Setup.Custom_Rule__c.Run_Validation_Rule__c, /* Nome do Usuario ou Profile para ignorar a Validation Rule */ 
     CASE( Priority, 
           &quot;Medium&quot;, 1,
           &quot;High&quot;, 1,
           0)
           = 1
         )</errorConditionFormula>
        <errorDisplayField>Priority</errorDisplayField>
        <errorMessage>Priority must be lower than Medium and High</errorMessage>
    </validationRules>
